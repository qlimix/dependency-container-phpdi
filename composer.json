{
    "name": "qlimix/dependency-container-phpdi",
    "type": "library",
    "description": "Dependency registry implemented with PHPDI",
    "license": "MIT",
    "keywords": [
        "qlimix",
        "dependency",
        "container",
        "phpdi"
    ],
    "authors": [
        {
            "name": "Frank",
            "email": "frank@qlimix.net"
        }
    ],
    "require": {
        "php": ">=7.2",
        "qlimix/dependency-container": "^1.0",
        "php-di/php-di": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Qlimix\\DependencyContainer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qlimix\\Tests\\DependencyContainer\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "qlimix/code-standard": "^1.0"
    }
}
